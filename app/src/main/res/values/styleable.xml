<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义圆角边框图片-->
    <declare-styleable name="RoundCornerImageView">
        <attr name="rc_border_color" format="color"/>
        <attr name="rc_border_width" format="dimension"/>
        <attr name="rc_press_alpha" format="integer"/>
        <attr name="rc_press_color" format="color"/>
        <attr name="rc_radius" format="dimension"/>
        <attr name="rc_shape_type" format="enum">
            <enum name="none" value="0"/>
            <enum name="round" value="1"/>
            <enum name="rectangle" value="2"/>
        </attr>
    </declare-styleable>
</resources>